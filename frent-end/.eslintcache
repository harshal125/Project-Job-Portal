      onSubmit={registerHandler}\n          className=\"checkout__inputs\"\n          noValidate\n        >\n          {!isFirstOpen ? (\n            <div className=\"checkout__personal\">\n              <h3>Personal Details</h3>\n              <div className=\"personal__inputs\">\n                <div className=\"personal__one\">\n                  <div>\n                    <p>First Name</p>\n                    <input\n                      required\n                      id=\"firstName\"\n                      placeholder=\"First name\"\n                      value={firstName}\n                      autoFocus\n                      onChange={(e) => setFirstName(e.target.value)}\n                    />\n                  </div>\n                  <div>\n                    <p>Last Name</p>\n                    <input\n                      required\n                      id=\"lastName\"\n                      placeholder=\"Last Name\"\n                      value={lastName}\n                      onChange={(e) => setLastName(e.target.value)}\n                    />\n                  </div>\n                </div>\n                <div className=\"personal__two\">\n                  <div>\n                    <p>Mobile Number</p>\n                    <input\n                      required\n                      id=\"mobileNumber\"\n                      placeholder=\"Mobile Number\"\n                      value={mobileNumber}\n                      onChange={(e) => setMobileNumber(e.target.value)}\n                    />\n                  </div>\n                  <div>\n                    <p>Date of Birth</p>\n                    <input\n                      id=\"date\"\n                      placeholder=\"DD/MM/YYYY\"\n                      type=\"date\"\n                      value={date}\n                      onChange={(e) => setDate(e.target.value)}\n                    />\n                  </div>\n                </div>\n                <div className=\"personal__three\">\n                  <div>\n                    <p>Country</p>\n                    <input\n                      required\n                      id=\"country\"\n                      placeholder=\"Country\"\n                      value={country}\n                      onChange={(e) => setCountry(e.target.value)}\n                    />\n                  </div>\n                  <div>\n                    <p>State/Province/Region</p>\n                    <input\n                      required\n                      id=\"state\"\n                      value={state}\n                      placeholder=\"State/Province/Region\"\n                      onChange={(e) => setState(e.target.value)}\n                    />\n                  </div>\n                </div>\n                <div className=\"personal__four\">\n                  <div>\n                    <p>City</p>\n                    <input\n                      required\n                      id=\"city\"\n                      placeholder=\"City\"\n                      value={city}\n                      onChange={(e) => setACity(e.target.value)}\n                    />\n                  </div>\n                  <div>\n                    <p>Zip/Postal code</p>\n                    <input\n                      required\n                      id=\"zip\"\n                      placeholder=\"Zip/Postal code\"\n                      value={zip}\n                      onChange={(e) => setZip(e.target.value)}\n                    />\n                  </div>\n                </div>\n                <div className=\"next__button\">\n                  <Button\n                    fullWidth\n                    variant=\"outlined\"\n                    color=\"primary\"\n                    onClick={nextHandle}\n                  >\n                    Next\n                  </Button>\n                </div>\n              </div>\n            </div>\n          ) : (\n            <div>\n              {!isSecondOpen ? (\n                <div className=\"checkout__edu\">\n                  <h3>Educational Details</h3>\n                  <div className=\"edu__inputs\">\n                    <div>\n                      <p>Highest Education</p>\n                      <input\n                        required\n                        id=\"education\"\n                        placeholder=\"Highest Education\"\n                        value={education}\n                        autoFocus\n                        onChange={(e) => setEducation(e.target.value)}\n                      />\n                    </div>\n                    <div>\n                      <p>Institute</p>\n                      <input\n                        required\n                        id=\"institute\"\n                        placeholder=\"Institute\"\n                        value={institute}\n                        onChange={(e) => setInstitute(e.target.value)}\n                      />\n                    </div>\n                    <div>\n                      <p>Specialization</p>\n                      <input\n                        required\n                        id=\"branch\"\n                        placeholder=\"Specialization\"\n                        value={branch}\n                        onChange={(e) => setBranch(e.target.value)}\n                      />\n                    </div>\n                    <div>\n                      <p>Year of Completion</p>\n                      <input\n                        required\n                        id=\"cvv\"\n                        placeholder=\"Year of Completion\"\n                        value={cvv}\n                        onChange={(e) => setCvv(e.target.value)}\n                      />\n                    </div>\n                    <div className=\"edu__button\">\n                      <Button\n                        fullWidth\n                        variant=\"outlined\"\n                        color=\"primary\"\n                        onClick={handlePreview}\n                      >\n                        Next\n                      </Button>\n                    </div>\n                  </div>\n                </div>\n              ) : (\n                // Preview Page\n\n                <div className=\"checkout__preview\">\n                  <h3>Review Your Information</h3>\n                  <div className=\"preview__inputs\">\n                    <div className=\"preview__key\">\n                      <p>First Name:</p>\n                      <p>Last Name:</p>\n                      <p>Mobile Number:</p>\n                      <p>Date:</p>\n                      <p>Country:</p>\n                      <p>State:</p>\n                      <p>City:</p>\n                      <p>Zip:</p>\n                      <p>Highest Education:</p>\n                      <p>Institute:</p>\n                      <p>Branch:</p>\n                      <p>Year of Completion:</p>\n                    </div>\n                    <div className=\"preview__value\">\n                      <p>{firstName}</p>\n                      <p>{lastName}</p>\n                      <p>{mobileNumber}</p>\n                      <p>{date}</p>\n                      <p>{country}</p>\n                      <p>{state}</p>\n                      <p>{city}</p>\n                      <p>{zip}</p>\n                      <p>{education}</p>\n                      <p>{institute}</p>\n                      <p>{branch}</p>\n                      <p>{cvv}</p>\n                    </div>\n                  </div>\n\n                  <div className=\"preview__button\">\n                    <Button type=\"submit\" variant=\"outlined\" color=\"primary\">\n                      Submit\n                    </Button>\n                  </div>\n                </div>\n              )}\n            </div>\n          )}\n        </form>\n      </div>\n\n      {/* Checkout Page Footer */}\n\n      <div className=\"c