    transition: \"filter 0.5s ease\",\n      },\n    };\n    const menu = [\"Explore\", \"Became a Creator\", \"Login\", \"Sign Up\"];\n    const menuItems = menu.map((val, index) => {\n      return (\n        <MenuItem\n          key={index}\n          delay={`${index * 0.1}s`}\n          onClick={() => {\n            this.handleLinkClick();\n          }}\n        >\n          {val}\n        </MenuItem>\n      );\n    });\n\n    return (\n      <div>\n        <div style={styles.container}>\n          <MenuButton\n            open={this.state.menuOpen}\n            onClick={() => this.handleMenuClick()}\n            color=\"black\"\n          />\n          {/* <div style={styles.logo}>Logo</div> */}\n        </div>\n        <Menu open={this.state.menuOpen}>{menuItems}</Menu>\n      </div>\n    );\n  }\n}\n\n/* MenuItem.jsx*/\nclass MenuItem extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      hover: false,\n    };\n  }\n\n  handleHover() {\n    this.setState({ hover: !this.state.hover });\n  }\n\n  render() {\n    const styles = {\n      container: {\n        opacity: 1,\n        animation: \"1s appear forwards\",\n        animationDelay: this.props.delay,\n      },\n      menuItem: {\n        fontFamily: `'Open Sans', sans-serif`,\n        fontSize: \"1.2rem\",\n        padding: \"1rem 0\",\n        // margin: \"0 5%\",\n        cursor: \"pointer\",\n        color: this.state.hover ? \"gray\" : \"#fafafa\",\n        transition: \"color 0.2s ease-in-out\",\n        animation: \"0.5s slideIn forwards\",\n        animationDelay: this.props.delay,\n      },\n      line: {\n        // width: \"30%\",\n        // height: \"1px\",\n        // background: \"gray\",\n        margin: \"0 auto\",\n        animation: \"0.5s shrink forwards\",\n        animationDelay: this.props.delay,\n      },\n    };\n    return (\n      <div style={styles.container}>\n        <div\n          style={styles.menuItem}\n          onMouseEnter={() => {\n            this.handleHover();\n          }}\n          onMouseLeave={() => {\n            this.handleHover();\n          }}\n          onClick={this.props.onClick}\n        >\n          {this.props.children}\n        </div>\n        <div style={styles.line} />\n      </div>\n    );\n  }\n}\n\n/* Menu.jsx */\nclass Menu extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      open: this.props.open ? this.props.open : false,\n    };\n  }\n\n  componentWillReceiveProps(nextProps) {\n    if (nextProps.open !== this.state.open) {\n      this.setState({ open: nextProps.open });\n    }\n  }\n\n  render() {\n    const styles = {\n      container: {\n        position: \"absolute\",\n        top: 0,\n        left: 0,\n        height: this.state.open ? \"280px\" : 0,\n        width: \"100vw\",\n        display: \"flex\",\n        flexDirection: \"column\",\n        background: \"black\",\n        opacity: 0.95,\n        color: \"#fafafa\",\n        transition: \"height 0.3s ease\",\n        zIndex: 2,\n      },\n      menuList: {\n        paddingTop: \"3rem\",\n      },\n    };\n    return (\n      <div style={styles.container}>\n        {this.state.open ? (\n          <div style={styles.menuList}>{this.props.children}</div>\n        ) : null}\n      </div>\n    );\n  }\n}\n\n/* MenuButton.jsx */\nclass MenuButton extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      open: this.props.open ? this.props.open : false,\n      color: this.props.color ? this.props.color : \"black\",\n    };\n  }\n\n  componentWillReceiveProps(nextProps) {\n    if (nextProps.open !== this.state.open) {\n      this.setState({ open: nextProps.open });\n    }\n  }\n\n  handleClick() {\n    this.setState({ open: !this.state.open });\n  }\n\n  render() {\n    const styles = {\n      container: {\n        height: \"32px\",\n        width: \"32px\",\n        display: \"flex\",\n        flexDirection: \"column\",\n        justifyContent: \"center\",\n        alignItems: \"center\",\n        cursor: \"pointer\",\n        padding: \"4px\",\n      },\n      line: {\n        height: \"2px\",\n        width: \"20px\",\n        background: this.state.color,\n        transition: \"all 0.2s ease\",\n      },\n      lineTop: {\n        transform: this.state.open ? \"rotate(45deg)\" : \"none\",\n        transformOrigin: \"top left\",\n        marginBottom: \"5px\",\n      },\n      lineMiddle: {\n        opacity: this.state.open ? 0 : 1,\n        transform: this.state.open ? \"translateX(-16px)\" : \"none\",\n      },\n      lineBottom: {\n        transform: this.state.open ? \"translateX(-1px) rotate(-45deg)\" : \"none\",\n        transformOrigin: \"top left\",\n        marginTop: \"5px\",\n      },\n    };\n    return (\n      <div\n        style={styles.container}\n        onClick={\n          this.props.onClick\n            ? this.props.onClick\n            : () => {\n                this.handleClick();\n              }\n        }\n      >\n        <div style={{ ...styles.line, ...styles.lineTop }} />\n        <div style={{ ...styles.line, ...styles.lineMiddle }} />\n        <div style={{ ...styles.line, ...styles.lineBottom }} />\n      </div>\n    );\n  }\n}\n\n/* Main.jsx */\nclass Main extends React.Component {\n  render() {\n    const styles = {\n      main: {\n        display: \"flex\",\n        flexDirection: \"column\",\n        alignItems: \"center\",\n        // height: \"50vh\",\n      },\n    };\n\n    return (\n      <div style={styles.main}>\n        <App />\n      </div>\n    );\n  }\n}\n\nexport default Main;\n","C:\\College-Minor-Project\\Job-portal-master\\Job-portal-master\\src\\components\\NavList\\Internship.js",[],"C:\\College-Minor-Project\\Job-portal-master\\Job-portal-master\\src\\components\\NavList\\Bookmark.js",[],"C:\\College-Minor-Project\\Job-portal-master\\Job-portal-master\\src\\components\\NavList\\Notification.js",[],"C:\\College-Minor-Project\\Job-portal-ma